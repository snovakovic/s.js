{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "templateStrings": true
  },
  "globals": {
  },
  "rules": {
    "no-console": 1,
    "quotes": [1, "single"],
    "eol-last": 1,
    "space-before-blocks": [1, "always"],
    "indent": [2, 2, {"SwitchCase": 1}]
  }
}
